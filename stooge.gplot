#! /usr/bin/gnuplot
set term postscript enhanced eps monochrome
set output "stooge.eps"
set xlabel "Size of array (slots)"
set ylabel "Average time taken (seconds)"
set pointsize 0.5
set key spacing 2
set nokey
set xrange [0:550]
set autoscale y
plot for [col=1:1] "trials.txt" using 1:col with points pointtype 5 title "Size of array vs average time taken to sort
